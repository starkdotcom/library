<link rel="stylesheet" href="/stylesheets/util.css" />
<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.0.8/css/all.css" />

<link rel="stylesheet" type="text/css"        href="https://cdnjs.cloudflare.com/ajax/libs/material-design-iconic-font/2.2.0/css/material-design-iconic-font.min.css" />
    
<div id="masthead">  
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          <h1><%= head%>
            <p class="lead"></p>
          </h1>
        </div>
         </div> 
    </div><!-- /cont -->
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          <div style="background-color: #116;" class="top-spacer"> </div>
        </div>
      </div> 
    </div><!-- /cont -->
    
  </div>
  
  
  <div class="container">
    <div class="row">
      
      <div class="col-md-12"> 
        
        <div style="background-color: #116;" class="panel">
          <div class="panel-body">
            
           
            
            <!--/stories-->
              <div class="row">
                <div class="limiter">
                    <div class="container-login100" style="background: #116;">
                        <div class="card form wrap-login100 p-l-55 p-r-55 p-t-53 p-b-54">
                            <form action="/login" method="post" class="login100-form validate-form" id="form">
                                <span class="login100-form-title p-b-45"> Login </span>
        
                                <div class="wrap-input100 validate-input m-b-3" data-validate="Username is reauired">
                                    <span class="label-input100">Username</span>
                                    <input id="username" class="input100" type="text" name="name"
                                        placeholder="Type your username" />
        
                                    <span class="focus-input100" data-symbol="&#xf15a;"></span>
                                </div>                        <div class="m-t-1 m-b-1">
                                    <span class="error" id="error"></span>
                                </div>
                                <div class="wrap-input100 validate-input" data-validate="Password is required">
                                    <span class="label-input100">Password</span>
                                    <span class="btn-show-pass">
                                        <i class="zmdi zmdi-eye" id="togglePassword"></i>
                                    </span>
                                    <input class="input100" id="password" type="password" name="password"
                                        placeholder="Type your password" />
        
                                    <span class="focus-input100" data-symbol="&#xf190;"></span>
                                </div>
                                <div class="m-t-1 m-b-1">
                                    <p id="strength-text"></p>
                                </div>
        
                                <div class="p-t-8 p-b-28 acc">
                                    <a style="float: left" href="/signup">
                                        Create a new account
                                    </a>
        
                                    <a style="float: right" href="#"> Forgot password? </a>
                                </div>
        
                                <div class="m-3 container-login100-form-btn">
                                    <div class="wrap-login100-form-btn">
                                        <div class="login100-form-bgbtn"></div>
                                        <button type="button" class="login100-form-btn" onclick="validateForm(redirect)">
                                            Login
                                        </button>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
           
          </div>
        </div>
                                                                                         
                                                      
                                                        
         </div><!--/col-12-->
    </div>
  </div>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
  
  <script src="/javascripts/script.js"></script>